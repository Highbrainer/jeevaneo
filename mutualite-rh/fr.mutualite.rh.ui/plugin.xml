<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Mutualité"
            id="fr.mutualite.rh.ui">
      </category>
      <view
            name="Exports Excel"
            icon="icons/sample.gif"
            category="fr.mutualite.rh.ui"
            class="fr.mutualite.rh.ui.views.ExportView"
            id="fr.mutualite.rh.ui.views.ExportView">
      </view>
      <view
            category="fr.mutualite.rh.ui"
            class="fr.mutualite.rh.ui.views.ImportView"
            icon="icons/sample.gif"
            id="fr.mutualite.rh.ui.views.ImportView"
            name="Imports">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="Mutualité"
            id="fr.mutualite.rh.ui">
      </category>
      <view
            name="Congés"
            icon="icons/sample.gif"
            category="fr.mutualite.rh.ui"
            class="fr.mutualite.rh.ui.views.CongesView"
            id="fr.mutualite.rh.ui.views.CongesView">
      </view>
      <view
            category="fr.mutualite.rh.ui"
            class="fr.mutualite.rh.ui.views.ImportView"
            icon="icons/sample.gif"
            id="fr.mutualite.rh.ui.views.ImportView"
            name="Imports">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="fr.mutualite.rh.ui.views.ExportView">
         </view>
         <view
               id="fr.mutualite.rh.ui.views.ImportView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
         <view
               id="fr.mutualite.rh.ui.views.CongesView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>

</plugin>
