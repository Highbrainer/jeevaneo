<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated chequedej -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="mutualite.rh.chequedej.presentation.ChequedejModelWizardID"
            name="%_UI_ChequedejModelWizard_label"
            class="mutualite.rh.chequedej.presentation.ChequedejModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ChequedejModelFile.gif">
         <description>%_UI_ChequedejModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated chequedej -->
      <editor
            id="mutualite.rh.chequedej.presentation.ChequedejEditorID"
            name="%_UI_ChequedejEditor_label"
            icon="icons/full/obj16/ChequedejModelFile.gif"
            extensions="chequedej"
            class="mutualite.rh.chequedej.presentation.ChequedejEditor"
            contributorClass="mutualite.rh.chequedej.presentation.ChequedejActionBarContributor">
      </editor>
   </extension>
   
   

   <extension
         point="org.eclipse.ui.popupMenus">   
      <objectContribution
            id="fr.mutualite.rh.chequedej.model.contribution1"
            objectClass="org.eclipse.emf.ecore.EObject">
         <menu
               id="fr.mutualite.rh.chequedej.model.menu1"
               label="Mutualite"
               path="additions">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="mutualite.rh.chequedej.popup.actions.NewCommandeAction"
               enablesFor="1"
               id="fr.mutualite.rh.chequedej.model.newCommandeAction"
               label="Préparer une nouvelle commande"
               menubarPath="fr.mutualite.rh.chequedej.model.menu1/group1">
         </action>
         <visibility>
         	<or>
         		<objectClass
                 name="mutualite.rh.chequedej.ChequeDej">
         		</objectClass>
         		<objectClass
                 name="mutualite.rh.chequedej.Commande">
         		</objectClass>
         	</or>
         </visibility>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="fr.mutualite.rh.chequedej.model.contribution2"
            objectClass="mutualite.rh.chequedej.ChequeDej">
         <menu
               id="fr.mutualite.rh.chequedej.model.menu1"
               label="Mutualite"
               path="additions">
            <separator
                  name="group">
            </separator>
         </menu>
         <action
               class="mutualite.rh.chequedej.popup.actions.InitNumerosAndSuccursalesAction"
               enablesFor="1"
               id="fr.mutualite.rh.chequedej.model.initNumerosSuccursalesAction"
               label="Initialiser les numéros et succursales"
               menubarPath="fr.mutualite.rh.chequedej.model.menu1/group">
         </action>
         <visibility>
            <or>
               <objectClass
                     name="mutualite.rh.chequedej.ChequeDej">
               </objectClass>
               <objectClass
                     name="mutualite.rh.chequedej.Parametrage">
               </objectClass>
            </or>
         </visibility>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="fr.mutualite.rh.chequedej.model.contribution3"
            objectClass="org.eclipse.emf.ecore.EObject">
         <menu
               id="fr.mutualite.rh.chequedej.model.menu1"
               label="Mutualite"
               path="additions">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="mutualite.rh.chequedej.popup.actions.ExportFormCommandeAction"
               enablesFor="1"
               id="fr.mutualite.rh.chequedej.model.exportFormCommandeAction"
               label="Générer les formulaires de saisie"
               menubarPath="fr.mutualite.rh.chequedej.model.menu1/group1">
         </action>
         <visibility>
            <or>
               <objectClass
                     name="mutualite.rh.chequedej.Commande">
               </objectClass>
            </or>
         </visibility>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="fr.mutualite.rh.chequedej.model.contribution4"
            objectClass="org.eclipse.emf.ecore.EObject">
         <menu
               id="fr.mutualite.rh.chequedej.model.menu1"
               label="Mutualite"
               path="additions">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="mutualite.rh.chequedej.popup.actions.ImportFormCommandeAction"
               enablesFor="1"
               id="fr.mutualite.rh.chequedej.model.importFormCommandeAction"
               label="Importer des formulaires de saisie"
               menubarPath="fr.mutualite.rh.chequedej.model.menu1/group1">
         </action>
         <visibility>
            <or>
               <objectClass
                     name="mutualite.rh.chequedej.Commande">
               </objectClass>
            </or>
         </visibility>
      </objectContribution>
   </extension>

</plugin>
